#ifndef MEMORY
#define MEMORY

#include <cstdio>
#include <iostream>
#include <vector>
using namespace std;



struct memory{
private:
	vector <int> a;
public:
	memory(int size=100);
	void Store(int adr, int value);
	int Retrieve(int adr);
	void copy(int sadr, int size, int aadr);
	void dump(int sadr, int range);
	void dump();

};
#endif
